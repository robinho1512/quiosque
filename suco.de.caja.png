	function parseVal(value, fracionada)
	{
		if(fracionada == '1'){
			return parseFloat(value);
		} else {
			return parseInt(value);
		}
	}

	function showVal(value, fracionada)
	{
		if(fracionada == '1'){
			return value.toFixed(3).replace('.', ',');
		} else {
			return value;
		}
	}

	function imprimeItemMesa(id_item, qtde)
	{
		$('#PrintScript').remove();
		$.getJSON( 'https://pastelariacariri.deliveryon.com.br/ajax/ajaxCreatePrintItem/'+ id_item +'/'+ qtde, function (response) {
			$('body').append(response);
			jsWebClientPrint.print();
