	}

	function adicionarProduto(obj)
	{
		if(!processando){
			
			processando = true;				
			var item = $(obj).parents('.box-products');
			var input_qtd = $(item).find('.input-qtd');
			var obs = $(item).find('.obs-produto');

			if(obs.length == 0){
				obs = '';
			} else {
				obs = $(obs).val();
			}

			$(item).children('.loader-cart').removeClass('hide');
			$(item).children('.btn-add-cart').addClass('hide');

			$.post( 'https://pastelariacariri.deliveryon.com.br/ajax/ajaxAddItemCart',
				{ deliveryT: '180d4a93a1e5ef5041fda3befa65f6e8',
				'id_produto': $(item).attr('data-id-produto'),
				'turno': '1',
				'qtd_produto': $(input_qtd).val(),
				'obs': obs }, function (r) {
					
				if(r['sucesso'] == 1){
					$('.qtd-itens-cart').text( ('00' + (parseInt($('.qtd-itens-cart').text()) + parseInt(r['qtd_adicional']))).slice(-2) );
					$('#vr-itens-cart').text( (parseFloat($('#vr-itens-cart').text().replace(',', '.')) + parseFloat(r['preco_adicional'])).toFixed(2).replace('.', ',') );
					
											abrirResumoCarrinho();
										
				} else {
					swal({
						title: 'ERRO!',
						text: r['msg_erro'],
						type: 'error',
						showConfirmButton: true,
						animation: false,
						customClass: 'add-cart-error',
						confirmButtonColor: 'red',
						html: true
					});															
				}

				processando = false;
				$(item).children('.loader-cart').addClass('hide');
				$(item).children('.btn-add-cart').removeClass('hide');
									
			}, 'json')
			  
			.fail(function( r ) {						
				console.log(r);
				
				swal({
					title: 'ERRO!',
					text: 'Ocorreu um erro que pode ter sido causado por uma falha na sua internet. Tente novamente em alguns minutos. Se persistir, por favor entre em contato.',
					type: 'error',
					showConfirmButton: true,
					animation: false,
					customClass: 'add-cart-error',
					confirmButtonColor: 'red'
				});

				processando = false;
				$(item).children('.loader-cart').addClass('hide');
				$(item).children('.btn-add-cart').removeClass('hide');
			});
		}
	}

	function atualizaSubtotal(operacao, valor, obj_input_qtd, fracionada)
	{
		var novo_subtotal = parseFloat($('#vr_subtotal').text().replace('.', '').replace(',', '.'));

		if(operacao == 'incrementa'){
			novo_subtotal += (fracionada == '1') ? (valor * 0.100) : valor;
		
		} else if(operacao == 'decrementa'){
			novo_subtotal -= (fracionada == '1') ? (valor * 0.100) : valor;
		
		} else if(operacao == 'diferenca-produto'){	
			var vr_unit = parseFloat($('#vr_unit').text().replace('.', '').replace(',', '.'));
			var qtd = obj_input_qtd.val().replace('.', '').replace(',', '.');

			novo_subtotal = qtd * valor;
		}

		$('#vr_subtotal').text( novo_subtotal.formatMoney(2, ',', '.') );
	}

	$('.input-qtd').blur(function(){
		if($(this).val() <= 0){
			alert('Informe uma quantidade válida.');
			$(this).select();
		} else {

							atualizaSubtotal('diferenca-produto', parseFloat($('#vr_produto').text().replace('.', '').replace(',', '.')), $('.input-qtd'));
					}
	});
	
	$('.input-qtd').click(function(){
		$(this).select();
	});	

	$('.remove-qtd').click(function(e){
		e.preventDefault();				

		var item = $(this).parents('.box-products');
		var input_qtd = $(item).find('.input-qtd');
		var fracionada = $(item).attr('data-fracionada');
		var min_value = (fracionada == '1') ? 0.100 : 1;
		var step_value = (fracionada == '1') ? 0.100 : 1;

								
			if(parseVal($(input_qtd).val().replace(',', '.'), fracionada) > min_value){
				$(input_qtd).val(showVal(parseVal($(input_qtd).val().replace(',', '.'), fracionada) - step_value, fracionada));
				old_qtd_produto = $(input_qtd).val();
				
				if($(this).parents('.resume-products').length){
					atualizaSubtotal('decrementa', parseFloat($('#vr_produto').text().replace('.', '').replace(',', '.')), $(input_qtd), fracionada);
				}
			}

			});

	$('.add-qtd').click(function(e){
		e.preventDefault();
		
		var item = $(this).parents('.box-products');
		var input_qtd = $(item).find('.input-qtd');
		var fracionada = $(item).attr('data-fracionada');
		var min_value = (fracionada == '1') ? 0.100 : 1;
		var step_value = (fracionada == '1') ? 0.100 : 1;

					
			$(input_qtd).val(showVal(parseVal($(input_qtd).val().replace(',', '.'), fracionada) + step_value, fracionada));
			old_qtd_produto = $(input_qtd).val();

			if($(this).parents('.resume-products').length){
				atualizaSubtotal('incrementa', parseFloat($('#vr_produto').text().replace('.', '').replace(',', '.')), $(input_qtd), fracionada);
			}

			});

	$('.btn-add-cart, .btn-build-product').click(function(e){
		e.preventDefault();
		var is_atendente = '0';

		var btnCart = this;
		if($(btnCart).parents('.resume-products').length){
	 		var item = $(btnCart).parents('.resume-products');
	 	} else {
	 		var item = $(btnCart).parents('.box-products');	 		
	 	}
		var qtd_produto = $(item).find('.input-qtd').val();
		var obs = $(item).find('.obs-produto').val();

		if(typeof(obs) == "undefined"){
			obs = '';
		}

		var status_entrega = verificaStatusEntrega();

		console.log('click');
				
		if(status_entrega.status == '0' && is_atendente == '0'){
			bootbox.alert(status_entrega.alerta);
		
		} else if(status_entrega.status == 1){
			bootbox.confirm(status_entrega.alerta, function(r){
				if(r){
					continuarAddCart(btnCart, item, qtd_produto, obs);
				}
			});

		} else {
			continuarAddCart(btnCart, item, qtd_produto, obs);
		}
	});

	function continuarAddCart(btnCart, item, qtd_produto, obs)
	{
		var usuario_deslogado = true;
		var is_atendente = false;
		var abre_personalizacao_direto = true;

		if(usuario_deslogado){
			var montar_produto = ($(btnCart).hasClass('btn-build-product')) ? 1 : 0;
			html_message = '<section class="container-form login-form-modal">'

							+ '<section class="login-form">'																

						 	+ '<h2>Faça Login</h2>'
							+ '<p>Já é nosso cliente? Preencha os dados abaixo para continuar comprando.</p>'

							+ '<form action="/login/meu_carrinho" method="POST">'
							+ '<input type="hidden" name="deliveryT" value="180d4a93a1e5ef5041fda3befa65f6e8" />'							

															+ '<input type="hidden" name="id_produto" value="'+ $(item).attr('data-id-produto') +'" />'
								+ '<input type="hidden" name="qtd_produto" value="'+ qtd_produto +'" />'
								+ '<input type="hidden" name="obs_produto" value="'+ $('.obs-produto').val() +'" />'
							 	+ '<input type="hidden" name="montar_produto" value="'+ montar_produto +'" />'
							
							 + '<div class="form-group">'
							 	+ '<label class="control-label">Email</label>'
							 	+ '<input type="text" name="email" class="form-control" />'
							 + '</div>'								 

							 + '<div class="form-group">'
							 	+ '<label class="control-label">Senha</label>'
							 	+ '<input type="password" name="senha" class="form-control" />'
							 + '</div>'

							 + '<input type="submit" name="submit" value="Entrar" class="btn btn-success" />'
							 + '<a href="/redefinir_senha" class="btn btn-link">Esqueci minha senha</a>'								 

						 + '</form></section>'
						 
						 + '<div class="both"></div>'
						 
						 /* + '<div class="divisoria-horizontal"></div>' */
						 
						 + '<div class="both"></div>'

						 + '<section class="login-register">'
							/*  + '<h2 class="cd5d3fc6cd">Cadastre-se</h2>'
							 + '<p>Para realizar compras em nosso site você precisa se cadastrar.</p>' */

							 + '<form action="/cadastro/passo1/produtos-ver-10" method="POST">'
							 + '<input type="hidden" name="deliveryT" value="180d4a93a1e5ef5041fda3befa65f6e8" />'							 								

															+ '<input type="hidden" name="id_produto" value="'+ $(item).attr('data-id-produto') +'" />'
							 	+ '<input type="hidden" name="qtd_produto" value="'+ qtd_produto +'" />'
								+ '<input type="hidden" name="obs_produto" value="'+ $('.obs-produto').val() +'" />'
							 	+ '<input type="hidden" name="montar_produto" value="'+ montar_produto +'" />'
														 
							 /* + '<input type="submit" name="cadastrar-modal" value="Quero me cadastrar" class="btn btn-primary btn-circle" />' */

						 + '</section>'

						 + '</form><div class="both"></div></section>';

			bootbox.dialog({
			  message: html_message,
			 
			});


		} else if($(btnCart).attr('data-mostrar-dialogo') == '1'){

			if(is_atendente && abre_personalizacao_direto){
				window.location.href = 'https://pastelariacariri.deliveryon.com.br/produtos/montar/'+ $(item).attr('data-id-produto') +'/'+ qtd_produto.replace(',', '.') +'/'+ encodeURIComponent(obs);
			} else {

				var html_message = "Esse produto pode ser personalizado:<br/>";

				if($(btnCart).attr('data-permite-adicionais') == 'S'){
					html_message += '<br/><strong>&bull; Com adicionais</strong></p>';
				}

				if($(btnCart).attr('data-permite-particoes') == 'S'){
					html_message += '<br/><strong>&bull; Pode ser dividido em sabores diferentes</strong></p>';
				}

				html_message += '<br/><br/>Deseja personalizar?';

				bootbox.dialog({
				  message: html_message,
				  title: "Informação",
				  buttons: {
				    info: {
				      label: "Adicionar ao carrinho",
				      className: "btn-danger",
				      callback: function() {
				      	adicionarProduto(btnCart);					      	
				      }
				    },
				    main: {
				      label: "Personalizar",
				      className: "btn-primary btnPersonalizar",
				      callback: function() {
				      	$('.btnPersonalizar').text('Carregando...');
						window.location.href = 'https://pastelariacariri.deliveryon.com.br/produtos/montar/'+ $(item).attr('data-id-produto') +'/'+ qtd_produto.replace(',', '.') +'/'+ encodeURIComponent(obs);
				      }
				    }
				  }
				});
			}

		} else {
			
			if($(btnCart).hasClass('btn-build-product')){
				window.location.href = 'https://pastelariacariri.deliveryon.com.br/produtos/montar/'+ $(item).attr('data-id-produto') +'/'+ qtd_produto.replace(',', '.') +'/'+ encodeURIComponent(obs);
			} else {
				if($(item).hasClass('resume-products')){
					$('#form').submit();
				} else {
					adicionarProduto(btnCart);
				}
			}
		}
	}
